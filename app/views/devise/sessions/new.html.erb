	<style type="text/css">

	</style>
	<link rel="stylesheet" href="https://css.zohocdn.com/salesiq/styles/floatbutton1_764bf172fd710d0d540b777a5a05ab02_.css">
	<link rel="stylesheet" href="https://css.zohocdn.com/salesiq/styles/floatbuttonpostload_9944f1d8c3bf8fbdf2e5b4cd91083d58_.css"> </head>

<body>
	<header class="sign-header">
		<p class="product-name">
			<a href="/us/books/"><img class="books-logo" src="https://www.zoho.com/books/images/books-black.svg" alt="Zoho Books - Accounting Software"></a>
		</p>
		<p class="sign-in">New User? <a href="/users/sign_up">Sign up</a></p>
	</header>
	<section class="testimonial-section">
		<div class="testimonial">
			<p class="testimonial-content">I've not seen anything as affordable and easy to use as Zoho Books. The reports are simple to run and can be scheduled to generate automatically. As a small business owner, I no longer worry about ageing receivables with the automatic payment reminders in Zoho Books.</p>
			<div class="customer-info"><img src="https://www.zoho.com/books/signup/images/stephen-circle.png" alt="Stephen J. Lalla - Zoho Books Customer - US" class="customer-img">
				<div class="customer-details">
					<p class="customer-name">Stephen J. Lalla</p>
					<p>CEO, Dynamic&nbsp;Image&nbsp;Marketing&nbsp;Systems, Inc., US</p>
				</div>
			</div>
		</div>
	</section>
	<section class="signup-section">
		<h1 class="signup-heading" id="signup-heading">ProfitView</h1>
		<div class="signup-container" id="zc-signup">
			<div class="ZI-form" style="opacity: 1;">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: 'login-form-tag signupform'} ) do |f| %>
          <div class="form-group za-email-container">
            <%= f.email_field :email, autofocus: true, autocomplete: "email",
                              class: 'bot-bor-design form-control', placeholder: 'Email' %>
          </div>

          <div class="form-group za-password-container">
            <%= f.password_field :password, autocomplete: "current-password",
                                 class: 'bot-bor-design form-control pwd-field', placeholder: 'Password' %>
            <%= image_tag 'visibility.png', class: 'pswd-visible-icon' %>
          </div>

          <div class='forgot-password pb-3'>
            <%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
              <small><%= link_to "Forgot Password?", new_password_path(resource_name), class: 'text-secondary' %></small>
            <% end %>
            <br>
            <small><%= link_to "New User? Sign up", new_user_registration_path, :class => 'text-secondary pt-2' %></small>
          </div>
          <% if false and devise_mapping.rememberable? %>
            <div class="field">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
            </div>
          <% end %>

          <div class="form-group">
            <%= f.submit "Sign In", class: 'btn-signup' %>
          </div>
        <% end %>
				<div class="form-group social">
					<div class="social-sign-in">or sign in using</div>
					<ul>
						<li><span alt="Google" title="Google" onclick="FederatedSignIn.GO('GOOGLE');zohoFedClickEvent('Google');" class="social-icon-google"></span></li>
						<li style="display: inline-block;"><span alt="Yahoo" title="Yahoo" onclick="FederatedSignIn.GO('YAHOO');zohoFedClickEvent('Yahoo');" class="social-icon-yahoo"></span></li>
						<li><span alt="Facebook" title="Facebook" onclick="FederatedSignIn.GO('FACEBOOK');zohoFedClickEvent('Facebook');" class="social-icon-fb"></span></li>
						<li><span alt="LinkedIn" title="LinkedIn" onclick="FederatedSignIn.GO('LINKEDIN');zohoFedClickEvent('Linkedin');" class="social-icon-linkedin"></span></li>
						<li><span alt="Twitter" title="Twitter" onclick="FederatedSignIn.GO('TWITTER');zohoFedClickEvent('Twitter');" class="social-icon-twitter"></span></li>
						<li><span alt="Office365" title="Office365" onclick="FederatedSignIn.GO('AZURE');zohoFedClickEvent('Office365');" class="social-icon-windows"></span></li>
					</ul>
				</div>
			</div>
		</div>
	</section>
	<section style="display:none" id="ZI-load">
		<div class="ZI-load"></div>
		<div class="ZI-popup">
			<h3>It's time to make your accounting really simple!</h3>
			<p class="ZI-con-desc">While we set up your account, please remember to verify your account by clicking on the link we sent to your registered email address.</p>
			<div class="ZI-loading">&nbsp;</div>
		</div>
	</section>

	<div class="zsiq_floatmain zsiq_theme1 siq_bR" data-id="zsalesiq" style="visibility: hidden; display: none;">
		<div id="zsiq_float" class="zsiq_float " style="font-family:inherit">
			<div class="zsiq_flt_rel"><em id="zsiq_agtpic" class="zsiq_user siqico-chat"></em>
				<div id="titlediv" class="zsiq_cnt">
					<div id="zsiq_maintitle" class="zsiq_ellips" title="We're Online!">We're Online!</div>
					<p id="zsiq_byline" class="zsiq_ellips" title="How may I help you today?">How may I help you today?</p><em class="siqico-close"></em></div><em id="zsiq_unreadcnt" class="zsiq_unrdcnt" style="display: none;"></em><em id="zsiq_avcall" class="zsiqmin_unrdcnt zsiq_unrdcnt siqico-mincall" style="display: none;"></em></div>
		</div>
	</div>
	<style id="zsiqcustomcss" data-id="zsalesiq">
	.zsiq_flt_rel {
		background-color: #0091ff !important;
	}

	.zsiq_seasonal .st2 {
		fill: #0091ff !important;
	}
	</style>
</html>
